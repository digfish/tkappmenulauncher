    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "../envlibloader"
            }
        },
        {
            "name": "Python: Current File as admin",
            "type": "f5anything",
            "request": "launch",
            "showTerminal": true,
            "command": "powershell -Command \"Start-Process -Wait  -Verb runAs python.exe ${file} \" ",
        }
    ]
}